<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="MemberFileMapper">
	<insert id="setWrite" parameterType="com.project.file.MemberFileVO">
		insert into memberFile values(member_seq.nextval, #{id}, #{fname}, #{oname})
	
	</insert>

	<select id="getSelect" parameterType="MemberVO" resultType="MemberVO">
		select * from memberFile where id=#{id}
	</select>

	<update id="profileUpdate" parameterType="java.lang.String">
		update memberFile set fname=#{fname}, oname=#{oname} where id=#{id}
		
	</update>
	


</mapper>
  
  
  
  
  
  
  
  
  